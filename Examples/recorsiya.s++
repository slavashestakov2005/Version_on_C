import types
def fact(n){
	println "N : " + n
	n = int(n - 1) + 1
	if n > 1 {
		println "Next rec"
		al = fact(n - 1)
		//n += 1 /* This code for what? */ 
		println "Pair : " + al + "\t" + n
		return n * al
	}
	else{
		println "End"
		n = -100
		return 1
	}
}

